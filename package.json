{
    "name": "elmer-ui-core",
    "version": "3.0.2",
    "description": "web app framework",
    "typings": "lib/index.d.ts",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "scripts": {
        "test": "mocha -t 30000  --reporter mochawesome",
        "cover": "nyc --reporter=html mocha",
        "lib": "tsc -w",
        "update": "elmer-cli uv",
        "start": "elmer-cli start",
        "build": "elmer-cli build"
    },
    "elmer-cli": {
        "hash": true,
        "optimization": false,
        "static": {
            "src": "./src",
            "des": "./lib"
        },
        "port": 3000,
        "template": "./example/index.html",
        "entry": "./example/index.ts",
        "output": {
            "filename": "[name].min.js",
            "publicPath": "",
            "chunkFilename": "chunks/[name]_[hash:8]_[id].min.js"
        }
    },
    "keywords": [
        "web framework",
        "elmer-ui-core",
        "typescript",
        "webApp"
    ],
    "author": "elmer s j mo",
    "license": "ISC",
    "nyc": {
        "include": [
            "src/**/*.ts",
            "src/**/*.tsx"
        ],
        "exclude": [
            "**/*.d.ts"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "iOS >= 7",
        "Android > 4.1",
        "Firefox > 20"
    ],
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-json-strings": "^7.13.8",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/preset-env": "^7.13.10",
        "@types/node": "^12.20.7",
        "autoprefixer": "^10.2.5",
        "babel-cli": "^6.26.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^5.1.2",
        "eslint-plugin-typescript": "^0.12.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "jsdom": "^15.2.0",
        "less": "^4.1.1",
        "less-loader": "^8.0.0",
        "mocha": "^6.2.1",
        "mochawesome": "^4.1.0",
        "nyc": "^14.1.1",
        "postcss": "^8.1.0",
        "postcss-loader": "^5.2.0",
        "schema-utils": "^2.5.0",
        "ts-loader": "^7.0.5",
        "ts-node": "^9.1.1",
        "tslint": "^5.11.0",
        "tslint-loader": "^3.5.4",
        "typescript": "^4.0.2",
        "typescript-eslint-parser": "^18.0.0"
    },
    "dependencies": {
        "@abraham/reflection": "^0.4.2",
        "@babel/polyfill": "^7.12.1",
        "@types/core-js": "^2.5.0",
        "axios": "^0.21.1",
        "chalk": "^2.4.1",
        "colors": "^1.3.3",
        "core-js": "^2.6.12",
        "decorator": "^0.0.1",
        "elmer-common": "^3.1.10",
        "elmer-loader": "^1.0.6",
        "elmer-redux": "^1.1.3",
        "elmer-validation": "^1.0.8",
        "elmer-virtual-dom": "^2.0.13",
        "elmer-worker": "^1.0.10",
        "reflect-metadata": "^0.1.13",
        "source-map": "^0.7.3",
        "zone.js": "^0.8.26"
    },
    "peerDependencies": {
        "elmer-loader": "^1.0.6",
        "elmer-redux": "^1.1.3",
        "elmer-validation": "^1.0.8",
        "elmer-worker": "^1.0.10"
    }
}
